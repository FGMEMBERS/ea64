<animation>
  <type>material</type>
  <object-name>gearF.001</object-name>
  <object-name>gearL.001</object-name>
  <object-name>gearR.001</object-name>
<condition><and>
<less-than>
        <property>/gear/gear[0]/position-norm</property>
<value>1</value>
</less-than>
<less-than>
        <property>/gear/gear[1]/position-norm</property>
<value>1</value>
</less-than>
<less-than>
        <property>/gear/gear[2]/position-norm</property>
<value>1</value>
</less-than></and>
</condition>
  <diffuse>
   <red>1</red>
   <green>1</green>
   <blue>1</blue>
  </diffuse>
  <ambient>
   <red>1</red>
   <green>1</green>
   <blue>1</blue>
  </ambient>
<emission>	
  <red>1</red>	
  <green>0</green>	
  <blue>0</blue>
 </emission>
 </animation>
<animation>
  <type>material</type>
  <object-name>gearFind</object-name>
  <object-name>gearLind</object-name>
  <object-name>gearRind</object-name>
<condition>
<and>
<equals>
        <property>/gear/gear[0]/position-norm</property>
<value>1</value>
</equals>
<equals>
        <property>/gear/gear[1]/position-norm</property>
<value>1</value>
</equals>
<equals>
        <property>/gear/gear[2]/position-norm</property>
<value>1</value>
</equals></and>
</condition>
  <diffuse>
   <red>1</red>
   <green>1</green>
   <blue>1</blue>
  </diffuse>
  <ambient>
   <red>1</red>
   <green>1</green>
   <blue>1</blue>
  </ambient>
<emission>	
  <red>0</red>	
  <green>1</green>	
  <blue>0</blue>
 </emission>
 </animation>
<animation>
  <type>pick</type>
  <object-name>handle</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>  
      <binding>
        <command>property-toggle</command>
        <property>/controls/gear/gear-down</property>
      </binding>
    </action>
  </animation>
<animation>
  <type>rotate</type>
  <object-name>handle</object-name>
        <property>/controls/gear/gear-down</property>
<factor>90</factor>
<offset-deg>-90</offset-deg>
<center>
<x-m>-0.00225</x-m>
<z-m>0.00027</z-m>
</center>
<axis><y>1</y></axis>
  </animation>
</PropertyList>
