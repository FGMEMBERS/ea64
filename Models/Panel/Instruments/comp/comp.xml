<?xml version="1.0"?>

<PropertyList>

	<path>comp.ac</path>

	<animation>
		<type>material</type>
		<object-name>Ring</object-name>
		<emission>
			<red>0.5</red>
			<green>0.5</green>
			<blue>0.5</blue>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>tige</object-name>
		<emission>
			<red>0.0</red>
			<green>0.5</green>
			<blue>0.0</blue>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Ring</object-name>
		<property>/instrumentation/magnetic-compass/indicated-heading-deg</property>
		<axis>
			<x>0.0</x>
			<y>0.0</y>
			<z>1.0</z>
		</axis>
		<center>
			<x-m> -0.00635 </x-m>
			<y-m>  0 </y-m>
			<z-m> -0.00789 </z-m>
		</center>
	</animation>

</PropertyList>
