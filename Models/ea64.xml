<PropertyList>

    <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/EchoAirPropPower/Models/Liveries", 10);

      var self = cmdarg();

      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
      var float_2 = self.getNode("sim/multiplay/generic/float[2]", 1);
      var float_3 = self.getNode("sim/multiplay/generic/float[3]", 1);
      var float_4 = self.getNode("sim/multiplay/generic/float[4]", 1);
      var float_5 = self.getNode("sim/multiplay/generic/float[5]", 1);

      var passenger = self.getNode("instrumentation/doors/passenger/position-norm", 1);
      var passengerstairs = self.getNode("instrumentation/doors/passengerstairs/position-norm", 1);
      var rearL = self.getNode("instrumentation/doors/rearL/position-norm", 1);
      var frontR = self.getNode("instrumentation/doors/rearR/position-norm", 1);
      var frontL = self.getNode("instrumentation/doors/rearL/position-norm", 1);
      var cockpit = self.getNode("instrumentation/doors/cockpit/position-norm", 1);

      passenger_door.alias(float_0);
      passengerstairs_door.alias(float_5);
      rearL_door.alias(float_1);
      frontR_door.alias(float_2);
      frontL_door.alias(float_3);
      cockpit_door.alias(float_4);
    </load>

    <unload>
      livery_update.stop();
      passenger_door.unalias();
      passengerstairs_door.unalias();
      rearL_door.unalias();
      frontR_door.unalias();
      frontL_door.unalias();
      cockpit_door.unalias();
    </unload>
    </nasal>
